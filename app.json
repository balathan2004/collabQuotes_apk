{
  "expo": {
    "name": "collab quotes",
    "slug": "jsmtutorial",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/index.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/index.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lightappdev.jsmtutorial"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/index.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/index.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "4d204e19-85d9-48c0-bc52-27b16cbc955a"
      }
    }
  }
}
